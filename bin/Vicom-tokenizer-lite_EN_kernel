#!/usr/bin/env ruby
$:.unshift(File.dirname(__FILE__) + '/../lib') unless $:.include?(File.dirname(__FILE__) + '/../lib')

require 'Vicom-tokenizer-lite_EN_kernel'

kernel = Opener::Kernel::Vicom::Tokenizer::Lite::EN.new
command = "#{kernel.command(:input=>ARGV[0], :arguments=>ARGV[1..-1])}"

puts `#{command}`
